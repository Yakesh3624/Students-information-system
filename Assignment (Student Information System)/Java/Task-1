public class student {
	int student_id;
	String first_name;
	String last_name; 
	String date_of_birth; 
	String email;
	long phone_number; 
}

public class course {
	int course_id; 
	String course_name; 
	int credits; 
	int teacher_id; 
}

public class enrollment {
	int enrollment_id; 
	int student_id; 
	int course_id; 
	String enrollment_date; 
}

public class teacher {
	int teacher_id;
	String first_name;
	String last_name ;
	String email;
}

public class payment {
	int payment_id; 
	int student_id; 
	int amount; 
	String payment_date; 
}
